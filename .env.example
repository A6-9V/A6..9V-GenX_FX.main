# AI Model Configuration
OPENAI_API_KEY=your_openai_api_key_here
MODEL_NAME=gpt-4
TEMPERATURE=0.7

# Database Configuration
DATABASE_URL=sqlite:///agentbrain.db
VECTOR_DB_PATH=./data/vector_db

# API Configuration
API_PORT=8000
API_HOST=localhost
SECRET_KEY=your_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=30

# EA Authentication (Expert Advisor API Keys)
# Single API key for EA authentication
EA_API_KEY=your_ea_api_key_here
# Or multiple API keys (comma-separated) for multiple EAs
# EA_API_KEYS=key1,key2,key3

# Vercel Sandbox Configuration (optional)
# For local development: run `vercel env pull` to get VERCEL_OIDC_TOKEN
# For production: Vercel manages these automatically
# VERCEL_OIDC_TOKEN=your_oidc_token_here
# VERCEL_TEAM_ID=your_team_id_here
# VERCEL_PROJECT_ID=your_project_id_here
# VERCEL_TOKEN=your_access_token_here
